{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Hulu Guzo",
  "version": "1.0.0",
  "identifier": "com.yonastedela63.huluguzo",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../.next"
  },
  "app": {
    "windows": [
      {
        "title": "hulu_guzo",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
